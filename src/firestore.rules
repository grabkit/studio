
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and make your data publicly accessible.
    // Anyone can read, write, and delete data in your database.
    // This is not recommended for production applications.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
